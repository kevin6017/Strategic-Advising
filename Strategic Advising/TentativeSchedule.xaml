<Page x:Class="Strategic_Advising.TentativeSchedule"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Strategic_Advising"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="TentativeSchedule"
      Loaded="Window_Loaded">

    <Grid Name="masterGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="res\SAB_TS.png"></ImageBrush>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" MinHeight="120"/>
        </Grid.RowDefinitions>

        <Button Name="thotButton" Content="THOT BUTTON" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="75" Grid.Row="0" Grid.Column="0" Margin="0,0,0,41" Click="homeButtonClick"/>

    </Grid>
</Page>
