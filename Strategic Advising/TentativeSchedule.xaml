<Page x:Class="Strategic_Advising.TentativeSchedule"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Strategic_Advising"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="TentativeSchedule"
      Loaded="Window_Loaded">

    <Grid Name="masterGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="res\SAB_TS.png"></ImageBrush>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="68"/>
            <RowDefinition Height="150"/>
        </Grid.RowDefinitions>
        <Button Name="thotButton" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="30" Width="80" Grid.Column="0" Margin="30,0,0,111" Click="BackToCompletedClasses_Button" Grid.Row="1"/>
        <Button Content="Home" HorizontalAlignment="Left" Margin="30,0,0,111" Grid.Row="1"  Grid.Column="0" VerticalAlignment="Bottom" Height="30" Width="80" Click="homeButtonClick"/>
    </Grid>
</Page>
